{
  "name": "@testpossessed/ts-substitute",
  "version": "1.1.1",
  "description": "A modern framework for creating substitutes from interfaces during TypeScript development based on the NSubstitute philosophy",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha dist/tests/**/*.test.js",
    "docgen": "typedoc",
    "build": "tsc",
    "prestart": "npm run build && npm run docgen",
    "start": "node_modules/.bin/lite-server --baseDir='./docs'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testpossessed/ts-substitute.git"
  },
  "keywords": [
    "substitute",
    "testing",
    "mock",
    "fake",
    "stub",
    "typescript"
  ],
  "author": "Mike Hanson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/testpossessed/ts-substitute/issues"
  },
  "homepage": "https://github.com/testpossessed/ts-substitute#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "chai": "^4.2.0",
    "lite-server": "^2.6.1",
    "mocha": "^8.2.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.14",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
